$(function(){function e(e){var n=e.indexOf("://")===-1?new URL("https://"+e):new URL(e);return n.hostname}function n(n){if(/^#/.test(n))return!0;var t=["github.com","stackoverflow.com","gitbook.cn"],o=e(n),r=o.indexOf(".csdn.net")>-1;return r=r?r:o.indexOf(".iteye.com")>-1,!!r||t.indexOf(o)>-1}function t(e){var n=/(http|https):\/\/(\w+:{0,1}\w*)?(\S+)(:[0-9]+)?(\/|\/([\w#!:.?+=&%!\-\/]))?/i;return!!n.test(e)}persentSet="undefined"!=typeof persentSet?persentSet:.3;var o=Math.random()<persentSet,r=window.articleId?articleId:fileName;$("#content_views").on("click","a",function(e){var i=$(this),d=i.attr("href");if(!t(d))return window.csdn.report.reportClick({mod:"popu_307",dest:d}),!0;if(o&&!n(d)){var w="//www.csdn.net/link?target_url="+encodeURIComponent(d)+"&id="+r+"&token="+i.data("token");return window.fileName?window.open(w,"_self"):window.open(w),window.csdn.report&&"function"==typeof window.csdn.report.reportClick&&window.csdn.report.reportClick({mod:"popu_307",dest:w}),!1}return window.fileName?(window.csdn.report&&"function"==typeof window.csdn.report.reportClick&&window.csdn.report.reportClick({mod:"popu_307",dest:d}),!0):(window.csdn.report&&"function"==typeof window.csdn.report.reportClick&&window.csdn.report.reportClick({mod:"popu_307",dest:d}),window.open(d,"_blank"),!1)})});